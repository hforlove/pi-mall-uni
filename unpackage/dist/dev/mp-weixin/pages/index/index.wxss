.index-swiper {
  height: 340rpx;
}
.index-swiper image {
  width: 100%;
  height: 100%;
}
.index-nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 40rpx 0;
}
.index-nav navigator {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.index-nav navigator image {
  width: 100rpx;
  height: 100rpx;
}
.index-nav navigator text {
  font-size: 24rpx;
}
.index-notice {
  height: 80rpx;
}
.index-notice swiper-item view {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #ed6a0c;
}
.index-notice swiper-item .iconfont {
  margin-right: 12rpx;
}

